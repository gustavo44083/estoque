{
   "type": "mysql",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/**/*.entity.ts"
   ],
   "migrations": [
      "src/migrations/**/*.ts"
   ],
   "subscribers": [
      "src/subscribers/**/*.ts"
   ],
   "cli": {
      "migrationsDir": "src/migrations",
      "subscribersDir": "src/subscribers"
   }
}
